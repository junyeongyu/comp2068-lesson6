<% include ../partials/header.ejs %>


<h1>Books</h1>

<table class="table table-striped table-hover">
<thead>
	<tr>
		<th>Title</th><th>Author</th><th>Price</th><th>Year</th>
	</tr>
</thead>
<tbody>
	<% books.forEach(function(book){ %>
	<tr>
		<td><%=book.title%></td>
		<td><%=book.author%></td>
		<td><%=book.price%></td>
		<td><%=book.year%></td>
	</tr>
	<% }); %>
</tbody>

</table>    
    
    
<% include ../partials/footer.ejs %>